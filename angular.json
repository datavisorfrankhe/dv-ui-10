{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "dv-ui": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/dv-ui/browser",
            "index": "src/index/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/static",
              "src/assets/favicon.ico",
              "src/app/modules/uml-essentials/assets",
              "src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/ngx-monaco-editor/assets/monaco",
                "output": "/assets/monaco"
              }
            ],
            "styles": [
              "src/index/styles.css",
              "src/scss/global.scss",
              "src/scss/global-light.scss",
              "src/scss/global-noir.scss",
              "node_modules/prismjs/themes/prism-okaidia.css"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/scss",
                "src/app/modules"
              ]
            },
            "scripts": [
              "src/static/echarts.boxplot.min.js",
              "node_modules/colresizable/colResizable-1.6.min.js",
              "node_modules/papaparse/papaparse.js",
              "node_modules/marked/lib/marked.js",
              "node_modules/prismjs/prism.js",
              "node_modules/prismjs/components/prism-css.min.js",
              "node_modules/tether/dist/js/tether.min.js"
            ]
          },
          "configurations": {
            "onprem": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.onprem.ts"
                },
                {
                  "replace": "src/index/index.html",
                  "with": "src/index/index.onprem.html"
                },
                {
                  "replace": "src/index/styles.css",
                  "with": "src/index/styles.onprem.css"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.production.ts"
                }
              ]
            },
            "staging": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "dv-ui:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "dv-ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dv-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "scripts": [
              "../dv-ui/node_modules/papaparse/papaparse.js"
            ],
            "styles": [
              "src/styles.css"
            ],
            "assets": [
              "src/static",
              "src/favicon.ico"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/scss"
              ]
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.app.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "dv-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dv-ui:serve:production"
            }
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/dv-ui/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/scss",
                "src/app/modules"
              ]
            }
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.production.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "dv-ui:build",
            "serverTarget": "dv-ui:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "dv-ui:build:production",
              "serverTarget": "dv-ui:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "dv-ui:build:production",
            "serverTarget": "dv-ui:server:production",
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {}
          }
        }
      }
    }
  },
  "defaultProject": "dv-ui",
  "cli": {
    "analytics": "dc7d57f9-b6a2-4d3a-8ffd-7ee859134b5a"
  }
}

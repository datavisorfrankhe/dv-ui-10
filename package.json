{
  "name": "dv-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "export NODE_OPTIONS=--max_old_space_size=8192 && ng serve --proxy-config proxy.conf.json --host 0.0.0.0 --disableHostCheck true",
    "build": "export NODE_OPTIONS=--max_old_space_size=8192 && ng build --aot --base-href /v3/ --configuration=$SERVER_ENV",
    "test": "ng test",
    "coverage": "ng test --code-coverage",
    "jenkins-test": "ng test --code-coverage --karma-config=karma.config.jenkins.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate-report": "export NODE_OPTIONS=--max_old_space_size=8192 && ng build --aot --base-href /v3/ --configuration=$SERVER_ENV --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "dev:ssr": "ng run dv-ui:serve-ssr",
    "serve:ssr": "node dist/dv-ui/server/main.js",
    "build:ssr": "export NODE_OPTIONS=--max_old_space_size=8192 && ng build --prod --aot && ng run dv-ui:server:production",
    "prerender": "ng run dv-ui:prerender"
  },
